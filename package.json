{
  "name": "kitchen-party",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config -- --environment=dev && ng serve --host 0.0.0.0 --port 4200",
    "prod": "npm run config -- --environment=prod && ng serve --configuration production --host 0.0.0.0 --port 4201",
    "build": "npm run config -- --environment=prod && ng build --configuration production --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "ngcc-test": "npx ngcc && jest --coverage",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "config": "ts-node ./scripts/setenv.ts",
    "netlify": "netlify dev",
    "netlify:backup": "netlify functions:invoke backup --port 1234"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.11",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "^14.2.11",
    "@angular/compiler": "^14.2.11",
    "@angular/core": "^14.2.11",
    "@angular/forms": "^14.2.11",
    "@angular/platform-browser": "^14.2.11",
    "@angular/platform-browser-dynamic": "^14.2.11",
    "@angular/router": "^14.2.11",
    "@angular/service-worker": "^14.2.11",
    "@google-cloud/firestore": "^5.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@ngxs/logger-plugin": "^3.7.3",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "dotenv": "^14.2.0",
    "firebase": "^9.6.3",
    "mailgun.js": "^5.0.2",
    "material-icons": "^1.10.6",
    "ng-mocks": "^13.0.3",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.1.2",
    "primeicons": "^6.0.1",
    "primeng": "^14.2.2",
    "rxjs": "~7.4.0",
    "stackdriver-errors-js": "^0.12.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.11",
    "@netlify/functions": "^1.0.0",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "chai": "^4.3.6",
    "core-js": "2.5.x",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-preset-angular": "^11.1.1",
    "netlify-cli": "^9.13.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.8.4"
  }
}
